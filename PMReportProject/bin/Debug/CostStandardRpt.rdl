    <DataSet Name="GetRptInfo">
      <Query>
        <DataSourceName>PMDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@KEY02">
            <Value>=Parameters!REPORT_ID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DATA_CHAR AS RptNo, DATA_CHAR2 AS CreatName
FROM SYSTEM_PARAMETER 
WHERE KEY01 = 'SLIP_NUMBER'
AND KEY02 = @KEY02</CommandText>
      </Query>
      <Fields>
