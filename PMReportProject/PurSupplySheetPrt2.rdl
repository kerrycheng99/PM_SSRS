        <Field Name="ACT_AMT">
          <DataField>ACT_AMT</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="TO_CUR">
          <DataField>TO_CUR</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TO_SUPPLY_TYPE">
          <DataField>TO_SUPPLY_TYPE</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="BUYER_CD">
